{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-autonumeric",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "autonumeric"
    ]
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    },
    "url": "https://developer.blackbaud.com"
  },
  "omnibar": {},
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "app": {
    "styles": [
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  }
}
