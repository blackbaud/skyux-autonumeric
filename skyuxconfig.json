{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-autonumeric",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "autonumeric"
    ]
  },
  "testSettings": {
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
